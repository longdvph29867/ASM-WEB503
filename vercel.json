{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      
      {
        "src": "/(.*)",
        "dest": "/"
      },
      { "handle": "filesystem" },
    { "src": "/v1/docs/(.*)", "dest": "/src/docs/$1" },
    { "src": "/v1/(.*)", "dest": "/src/routes/$1" },
    { "src": ".*", "dest": "/index.js" }
    ]
  }